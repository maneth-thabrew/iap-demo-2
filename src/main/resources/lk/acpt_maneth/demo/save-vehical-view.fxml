<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.acpt_maneth.demo.VehicalViewController">
   <children>
      <Label layoutX="214.0" layoutY="14.0" prefHeight="39.0" prefWidth="171.0" text="Save Vehical" textFill="#fa0606">
         <font>
            <Font name="System Bold" size="27.0" />
         </font>
      </Label>
      <TextField fx:id="txtBrand" layoutX="50.0" layoutY="83.0" prefHeight="26.0" prefWidth="310.0" promptText="Enter Brand" />
      <TextField fx:id="txtGears" layoutX="50.0" layoutY="156.0" prefHeight="26.0" prefWidth="310.0" promptText="Enter NoOfGears" />
      <TextField fx:id="txtModel" layoutX="50.0" layoutY="119.0" prefHeight="26.0" prefWidth="310.0" promptText="Enter Model" />
      <TextField fx:id="txtPrice" layoutX="50.0" layoutY="200.0" prefHeight="26.0" prefWidth="310.0" promptText="Enter Price" />
      <Button layoutX="427.0" layoutY="106.0" mnemonicParsing="false" onAction="#saveBtn" prefHeight="26.0" prefWidth="85.0" text="Save" textFill="#0d0d0d" />
      <Button layoutX="427.0" layoutY="187.0" mnemonicParsing="false" onAction="#cancelBtn" prefHeight="26.0" prefWidth="85.0" text="Cancel" />
   </children>
</AnchorPane>
